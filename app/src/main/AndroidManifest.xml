<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.trademetest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TradeMeTest">
        <activity
            android:name=".SingleListing"
            android:label="@string/title_activity_single_listing"
            android:theme="@style/Theme.TradeMeTest.NoActionBar"></activity>
        <activity android:name=".refine" />
        <activity
            android:name=".listingView"
            android:label="@string/title_activity_listing_view"
            android:theme="@style/Theme.TradeMeTest.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>